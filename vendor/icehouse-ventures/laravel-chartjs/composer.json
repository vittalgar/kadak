{
    "name": "icehouse-ventures/laravel-chartjs",
    "description": "Simple package to facilitate and automate the use of charts in Laravel using the Chart.js library",
    "keywords": [
        "laravel",
        "chartjs",
        "charts",
        "graphs",
        "icehouseventures"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Icehouse Ventures",
            "email": "tech@icehouseventures.co.nz"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "IcehouseVentures\\LaravelChartjs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IcehouseVentures\\LaravelChartjs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IcehouseVentures\\LaravelChartjs\\Providers\\ChartjsServiceProvider"
            ],
            "aliases": {
                "Chartjs": "IcehouseVentures\\LaravelChartjs\\Facades\\Chartjs"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.19|^10.0",
        "pestphp/pest": "^2.30|^3.7",
        "laravel/pint": "^1.13",
        "phpstan/phpstan": "^1.10|^2.1",
        "pestphp/pest-plugin-laravel": "^2.2|^3.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
